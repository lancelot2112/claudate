{
  "name": "claudate",
  "version": "1.0.0",
  "description": "An agentic team framework that orchestrates specialized AI agents for collaborative software development and problem-solving",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "test": "jest",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:e2e": "jest --testPathPattern=tests/e2e",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "db:migrate": "npx prisma migrate dev",
    "db:seed": "npx prisma db seed",
    "db:reset": "npx prisma migrate reset",
    "db:generate": "npx prisma generate",
    "agents:start": "node dist/scripts/start-agents.js",
    "agents:stop": "node dist/scripts/stop-agents.js",
    "agents:test": "jest --testPathPattern=tests/unit/agents",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "clean": "rimraf dist",
    "postinstall": "npm run db:generate"
  },
  "keywords": [
    "ai",
    "agents",
    "multi-agent",
    "gemini",
    "claude",
    "communication",
    "executive",
    "automation"
  ],
  "author": "Claudate Team",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.3",
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^7.2.4",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/websocket": "^8.3.1",
    "@google-ai/generativelanguage": "^2.9.1",
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^5.6.0",
    "@sendgrid/mail": "^7.7.0",
    "bcrypt": "^5.1.1",
    "bull": "^4.12.2",
    "bullmq": "^4.15.4",
    "canvas": "^2.11.2",
    "chart.js": "^4.4.0",
    "compression": "^1.7.4",
    "d3": "^7.8.5",
    "dotenv": "^16.3.1",
    "fastify": "^4.24.3",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.2",
    "google-auth-library": "^9.4.0",
    "googleapis": "^129.0.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "sharp": "^0.32.6",
    "tsconfig-paths": "^4.2.0",
    "twilio": "^4.19.3",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/d3": "^7.4.3",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/pg": "^8.10.7",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "prisma": "^5.6.0",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "prisma": {
    "seed": "ts-node scripts/seed.ts"
  }
}
